{% extends 'base.html.twig' %}

{% block body %}
    <h1>Commandeclient</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ commandeClient.id }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{% if commandeClient.date %}{{ commandeClient.date|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('commandeclient_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('commandeclient_edit', { 'id': commandeClient.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
