{% extends 'base.html.twig' %}

{% block body %}
    <h1>Secteurs list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Ville</th>
                <th>Numero</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for secteur in secteurs %}
            <tr>
                <td><a href="{{ path('secteur_show', { 'id': secteur.id }) }}">{{ secteur.id }}</a></td>
                <td>{{ secteur.ville }}</td>
                <td>{{ secteur.numero }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('secteur_show', { 'id': secteur.id }) }}">show</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    
{% endblock %}
